def interface():
    print("Blood Calculator")
    print("Options:")
    print("9 - Quit")
    keep_running = True
    while keep_running:
        choice = input("Enter choice: ")
        if choice == "9":
            return

def input_HDL():
    inp = input("Enter HDL value:")
    return int(inp)

def check_HDL(HDL):
    if HDL >= 60:
        return "Normal"
    elif HDL < 40:
        return "Low"
    else:
        return "Borderline Low"

interface()